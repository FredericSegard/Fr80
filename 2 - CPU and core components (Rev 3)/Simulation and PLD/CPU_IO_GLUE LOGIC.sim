PIN   7 =  A4 
PIN   8 =  A5 
PIN   9 =  A6 
PIN  11 =  A7 
PIN  14 =  BANKRD 
PIN  15 =  BANKWR 
PIN  18 =  CTCCS 
PIN   1 =  IORQ 
PIN  16 =  KEYRD 
PIN  17 =  KEYWR 
PIN   2 =  M1 
PIN  13 =  MODE1 
